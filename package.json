    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.5",
    "fast-csv": "^3.4.0",
    "joi": "^14.3.1",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-gottlieb/scrabble.git"
