{
  "name": "scrabble",
  "version": "1.0.0",
  "description": "Scrabble remake",
  "engines": {
    "node": "12.11.1"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
